{
    "name": "landsman/betterstack-symfony",
    "description": "Demo implementation of BetterStack to Symfony framework",
    "type": "project",
    "require": {
        "php": "^7.4.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/var-dumper": "5.4.*",
        "symfony/yaml": "5.4.*",
        "logtail/monolog-logtail": "^2.0.0",
        "symfony/runtime": "5.4.*",
        "symfony/dotenv": "5.4.*"
    },
    "require-dev": {
        "symfony/web-profiler-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/debug-bundle": "5.4.*",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
